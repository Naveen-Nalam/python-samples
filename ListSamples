#list manipulation
list1 = ['abcd',786, 2.23, 'john']
#print the list details
print (list1)
print (list1[0])
print (list1[1:3])
print (list1[2:])
print ('List Data', list1)

list1.append("appedened value")

print ('After appending',list1)

list1.clear()
print ('After clearing list data',list1)

list1=["value"]
print (list1)
print ("Counting list value",list1.count(''))



 
